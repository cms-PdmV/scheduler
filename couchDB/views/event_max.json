{
   "_id": "_design/event_max",
   "language": "javascript",
   "views": {
       "eventMaxAll": {
           "map": "function(doc) {
              if (doc.type == "eventMax")
              { emit(null, doc);
              }
            }"
       },
       "eventMax": {
           "map": "function(doc) {
              if (doc.type == "eventMax") {
                  emit(doc._id, [doc._id, doc._rev]);
              }
            }"
       }
   }
}
